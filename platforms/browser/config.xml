<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ilpc.mobile" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>ILPC Mobile</name>
    <description>
        ILPC Mobile is a mobile apps for ILPC UBAYA.
    </description>
    <author email="si.ilpc.ubaya@gmail.com" href="http://ilpc.ifubaya.com">
        ILPC 2019 Information System Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
